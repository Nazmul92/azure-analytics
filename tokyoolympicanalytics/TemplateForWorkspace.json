			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT TOP 10 Discipline, COUNT(*) AS AthletesCount\nFROM athletes\nGROUP BY Discipline\nORDER BY AthletesCount DESC;",
