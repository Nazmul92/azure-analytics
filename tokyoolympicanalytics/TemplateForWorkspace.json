				"type": "ManagedIdentity",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Correlation_athletes_total_madels')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT a.Country, COUNT(a.PersonName) AS AthleteCount, m.Total AS TotalMedals\nFROM athletes a\nJOIN medals m ON a.Country = m.TeamCountry\nGROUP BY a.Country, m.Total\nORDER BY TotalMedals DESC;\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Highest_female_participation_by_discipline')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT Discipline, Female, Total,\n(Female*1.0/Total)*100 AS FemalePercentage\nFROM entriesgender\nWHERE Female>Male\nORDER BY FemalePercentage DESC;",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Male_female_percentage_by_discipline')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT Discipline, Female, Male, Total,\n (Female*1.0/Total)*100 AS FemalePercentage,\n (Male*1.0/Total)*100 AS MalePercentage\nFROM entriesgender\nORDER BY Total;",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Team_count_by_country')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT Country, COUNT(DISTINCT TeamName) AS TeamCount\nFROM teams\nGROUP BY Country\nORDER BY TeamCount DESC;\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Top_10_country_by_athletes_count')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT TOP 10 Country, COUNT(*) AS AthletesCount\nFROM athletes\nGROUP by Country\nORDER BY AthletesCount DESC;",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Top_10_country_by_gold')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "SELECT TOP 10 TeamCountry, Gold\nFROM medals\nORDER BY Gold DESC;\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "tokyo_olympic_db",
